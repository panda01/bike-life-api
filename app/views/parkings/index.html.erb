<h1>Listing parkings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Israck</th>
      <th>Racks</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parkings.each do |parking| %>
      <tr>
        <td><%= parking.name %></td>
        <td><%= parking.isRack %></td>
        <td><%= parking.racks%></td>
        <td><%= parking.latitude %></td>
        <td><%= parking.longitude %></td>
        <td><%= parking.address %></td>
        <td><%= link_to 'Show', parking %></td>
      </tr>
    <% end %>
  </tbody>
</table>

